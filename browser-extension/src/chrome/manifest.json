{
  "name": "Morphs.io chrome extension",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*morphic:*"],
      "js": ["content.js"]
    }
  ],
  "externally_connectable": {
    "matches": ["https://morphs.io/*"]
  },
  "minimum_chrome_version": "88"
}
