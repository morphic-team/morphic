{
  "name": "morphic-website",
  "version": "1.3.0",
  "scripts": {
    "start": "ng serve --host 0.0.0.0",
    
    "dev:serve": "ng serve --host 0.0.0.0",
    "dev:watch": "ng build --watch --configuration development",
    
    "build": "npm run build:assets && ng build",
    "build:dev": "npm run build:assets && ng build --configuration development",
    "build:prod": "npm run build:assets && ng build --configuration production",
    "build:assets": "node scripts/process-changelogs.js",
    
    "test": "ng test",
    "test:watch": "ng test --watch",
    
    "clean": "rm -rf dist/ public/changelogs/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.2",
    "@angular/common": "^20.0.2",
    "@angular/compiler": "^20.0.2",
    "@angular/core": "^20.0.2",
    "@angular/forms": "^20.0.2",
    "@angular/platform-browser": "^20.0.2",
    "@angular/platform-browser-dynamic": "^20.0.2",
    "@angular/router": "^20.0.2",
    "@popperjs/core": "^2.11.8",
    "@types/leaflet": "^1.9.18",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^3.2.0",
    "ngx-pagination": "^6.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.2",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "^5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  }
}
